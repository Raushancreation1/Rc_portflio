# Copy this file to .env.local and fill in your secrets.
# Note: .env.local is gitignored. Do not commit your real credentials.

# MongoDB connection string. For Atlas, it looks like:
# mongodb+srv://<username>:<password>@<cluster>.mongodb.net/?retryWrites=true&w=majority
MONGODB_URI=

# Database name to use (e.g., "portfolio")
MONGODB_DB=
